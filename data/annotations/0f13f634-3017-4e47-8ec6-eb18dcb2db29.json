      "uuid": "ae27ba7c-085a-4d61-8c8c-76d2e93b24f7"
    },
    {
      "start_time": 27,
      "end_time": 28,
      "annotation": [
        {
          "type": "paragraph",
          "children": [
            {
              "text": "despite it all."
            }
          ]
        }
      ],
      "tags": [
        {
          "category": "_uncategorized_",
          "tag": "Transcription"
        }
      ],
      "uuid": "79a4b977-4c59-45ee-8a9b-48ec6d8d6e85"
    },
    {
      "start_time": 29,
      "end_time": 41,
      "annotation": [
        {
          "type": "paragraph",
          "children": [
            {
              "text": "black and white, Asian, Spanish, Native American, pretty, plain, thin, fat loud or celibate, we rise."
            }
          ]
        }
      ],
      "tags": [
        {
          "category": "_uncategorized_",
          "tag": "Transcription"
        },
