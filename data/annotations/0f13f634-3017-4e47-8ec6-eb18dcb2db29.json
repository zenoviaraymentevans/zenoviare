    {
      "start_time": 54,
      "end_time": 56,
      "annotation": [
        {
          "type": "paragraph",
          "children": [
            {
              "text": "does my sassiness upset you?"
            }
          ]
        }
      ],
      "tags": [
        {
          "category": "_uncategorized_",
          "tag": "Transcription"
        }
      ],
      "uuid": "80f677ba-673a-4324-ae52-39b14d19543d"
