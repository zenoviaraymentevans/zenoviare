      "uuid": "a78de6d7-06d5-40ac-b386-c683c0aafa7d"
    },
    {
      "start_time": 130,
      "end_time": 132,
      "annotation": [
        {
          "type": "paragraph",
          "children": [
            {
              "text": "up from a past rooted in pain i rise"
            }
          ]
        }
      ],
      "tags": [
        {
          "tag": "Transcription",
          "category": "Background"
        },
        {
          "category": "_uncategorized_",
          "tag": "tempo"
        }
      ],
      "uuid": "0919499f-0c5b-4322-8909-1567bbf712ba"
    },
    {
      "start_time": 133,
      "end_time": 136,
      "annotation": [
        {
          "type": "paragraph",
          "children": [
            {
              "text": "I'm a black ocean, leaping and wide,"
            }
          ]
        }
      ],
      "tags": [
        {
          "tag": "Transcription",
          "category": "Background"
        },
        {
          "category": "_uncategorized_",
          "tag": "tempo"
        }
      ],
      "uuid": "b234f5b7-13ff-49f2-a92a-35bd3bbf88d2"
    },
    {
      "start_time": 137,
      "end_time": 2,
      "annotation": [
        {
          "type": "paragraph",
          "children": [
            {
              "text": "Welling and swelling I bear in the tide."
            }
          ]
        }
      ],
      "tags": [
        {
          "tag": "Transcription",
          "category": "Background"
        },
        {
          "category": "_uncategorized_",
          "tag": "tempo"
        }
      ],
      "uuid": "3ca0b231-86e9-4777-82b9-8918539b775b"
    },
    {
      "start_time": 142,
      "end_time": 144,
      "annotation": [
        {
          "type": "paragraph",
          "children": [
            {
              "text": "Leaving behind nights of terror and fear"
            }
          ]
        }
      ],
      "tags": [
        {
          "tag": "Transcription",
          "category": "Background"
        },
        {
          "tag": "Tempo",
