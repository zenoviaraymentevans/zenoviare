      "end_time": 155,
      "annotation": [
        {
          "type": "paragraph",
          "children": [
            {
              "text": "Bringing the gifts that my ancestors gave,"
            }
          ]
        }
      ],
      "tags": [
        {
          "tag": "Transcription",
          "category": "Background"
        },
        {
          "tag": "Tempo",
          "category": "Performance"
