    },
    {
      "start_time": 86,
      "end_time": 89,
      "annotation": [
        {
          "type": "paragraph",
          "children": [
            {
              "text": "does my sassiness upset you?"
            }
          ]
        }
      ],
      "tags": [
        {
          "category": "_uncategorized_",
          "tag": "Transcription"
        },
        {
          "category": "_uncategorized_",
          "tag": "speech"
